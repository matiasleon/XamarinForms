<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="hxTest.Views.Employees.DetailEmployeePage"
             Title="Edicion">
    <ContentPage.Content>
        <StackLayout>
            <Entry Text="{Binding Employee.Office}"></Entry>
            <Entry Text="{Binding Employee.Username}"></Entry>
            <Button Text="Actualizar" Command="{Binding UpdateCommand}"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>